//Time Complexity = O(n) and Space Complexity = O(1)
class Solution {
public:
    int removeElement(vector<int>& nums, int val) {
        int i,count=0;
        for(i=0;i<nums.size();i++){
            if(nums[i]!=val){
                nums[count]=nums[i];
                count++;
            }
        }
        return count;
    }
};
